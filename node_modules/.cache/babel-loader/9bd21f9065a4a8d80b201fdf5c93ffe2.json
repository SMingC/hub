{"ast":null,"code":"import RequestAccount from \"@/components/group/signIn/RequestAccount.vue\";\nimport ThemeSwitch from \"@/components/button/ThemeSwitch.vue\";\nimport Notification from \"@/components/group/signIn/Notification.vue\";\nimport Header from \"@/components/Headers/Header.vue\";\nimport * as netlifyIdentityWidget from \"netlify-identity-widget\";\nimport { auth } from \"@/main\";\nexport default {\n  name: \"SignIn\",\n  components: {\n    RequestAccount,\n    ThemeSwitch,\n    Notification,\n    Header\n  },\n\n  data() {\n    return {\n      email: null,\n      password: null,\n      hasText: false,\n      text: \"\"\n    };\n  },\n\n  computed: {\n    isDarkMode() {\n      return this.$store.getters.isDarkMode;\n    }\n\n  },\n  methods: {\n    onSubmit() {\n      const email = this.email;\n      const password = this.password;\n      auth.login(email, password).then(res => {\n        alert(\"===sigin success ===\" + res);\n      }).catch(err => {\n        alert(\"===bad sigin\" + err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8CA;AACA;AACA;AACA;AACA;AAEA;AAEA;EACAA,cADA;EAEAC;IACAC,cADA;IAEAC,WAFA;IAGAC,YAHA;IAIAC;EAJA,CAFA;;EAQAC;IACA;MACAC,WADA;MAEAC,cAFA;MAGAC,cAHA;MAIAC;IAJA;EAMA,CAfA;;EAgBAC;IACAC;MACA;IACA;;EAHA,CAhBA;EAqBAC;IACAC;MACA;MACA;MAEAC,KACAC,KADA,CACAT,KADA,EACAC,QADA,EAEAS,IAFA,CAEAC;QACAC;MACA,CAJA,EAKAC,KALA,CAKAC;QACAF;MACA,CAPA;IAQA;;EAbA;AArBA","names":["name","components","RequestAccount","ThemeSwitch","Notification","Header","data","email","password","hasText","text","computed","isDarkMode","methods","onSubmit","auth","login","then","res","alert","catch","err"],"sourceRoot":"src/views","sources":["SignIn.vue"],"sourcesContent":["<template>\n  <div>\n    <Header />\n    <div\n      class=\"container\"\n      :class=\"{\n        'light-background': !isDarkMode,\n        'dark-background': isDarkMode,\n      }\"\n    >\n      <Notification v-if=\"hasText\" :text=\"text\" />\n      <RequestAccount />\n      <div class=\"login\">\n        <img src=\"@/assets/svg/DCHQ.svg\" v-show=\"isDarkMode\" />\n        <img src=\"@/assets/svg/DCHQ-dark.svg\" v-show=\"!isDarkMode\" />\n        <h4 :class=\"{ 'light-text': isDarkMode, 'dark-text': !isDarkMode }\">\n          登录到 NCHU_200hub\n        </h4>\n        <form @submit.prevent=\"onSubmit\">\n          <input\n            type=\"email\"\n            placeholder=\"Email\"\n            :class=\"{ 'light-field': isDarkMode, 'dark-field': !isDarkMode }\"\n            v-model=\"email\"\n            required\n          />\n          <input\n            type=\"password\"\n            placeholder=\"Password\"\n            :class=\"{ 'light-field': isDarkMode, 'dark-field': !isDarkMode }\"\n            v-model=\"password\"\n            required\n          />\n          <button style=\"cursor: pointer\">Sign In</button>\n        </form>\n        <router-link\n          to=\"/recover\"\n          :class=\"{ 'light-link': isDarkMode, 'dark-link': !isDarkMode }\"\n          >Forgot your password?</router-link\n        >\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport RequestAccount from \"@/components/group/signIn/RequestAccount.vue\";\nimport ThemeSwitch from \"@/components/button/ThemeSwitch.vue\";\nimport Notification from \"@/components/group/signIn/Notification.vue\";\nimport Header from \"@/components/Headers/Header.vue\";\nimport * as netlifyIdentityWidget from \"netlify-identity-widget\";\n\nimport { auth } from \"@/main\";\n\nexport default {\n  name: \"SignIn\",\n  components: {\n    RequestAccount,\n    ThemeSwitch,\n    Notification,\n    Header,\n  },\n  data() {\n    return {\n      email: null,\n      password: null,\n      hasText: false,\n      text: \"\",\n    };\n  },\n  computed: {\n    isDarkMode() {\n      return this.$store.getters.isDarkMode;\n    },\n  },\n  methods: {\n    onSubmit() {\n      const email = this.email;\n      const password = this.password;\n\n      auth\n        .login(email, password)\n        .then((res) => {\n          alert(\"===sigin success ===\" + res);\n        })\n        .catch((err) => {\n          alert(\"===bad sigin\" + err);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n@import \"@/global-styles/colors.scss\";\n@import \"@/global-styles/typography.scss\";\n.container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  min-height: 100vh;\n  padding-top: 16px;\n  padding-bottom: 24px;\n  position: relative;\n}\n\n.login {\n  width: 400px;\n  text-align: center;\n  margin-left: 16px;\n  margin-right: 16px;\n}\n\n.github-container {\n  width: 100%;\n  display: flex;\n  justify-content: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}