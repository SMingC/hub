{"ast":null,"code":"import * as NetlifyIdentityWidget from \"netlify-identity-widget\";\nimport VideoTopMenu from \"@/components/sections/movieHome_section/VideoTopMenu.vue\";\nimport VideoCarousels from \"@/components/sections/movieHome_section/VideoCarousels.vue\";\nimport VideoTV from \"@/components/sections/movieHome_section/VideoTV.vue\";\nimport AllSortsVideo from \"@/components/sections/movieHome_section/AllSortsVideo.vue\";\nimport FooterVideoHome from \"@/components/group/movieHome/footer/FooterVideoHome.vue\";\nexport default {\n  components: {\n    VideoTopMenu,\n    VideoCarousels,\n    AllSortsVideo,\n    VideoTV,\n    FooterVideoHome\n  },\n\n  created() {\n    if (document.body.clientWidth <= 900) {\n      this.isOnComputer = false;\n    }\n  },\n\n  mounted() {\n    NetlifyIdentityWidget.open();\n  },\n\n  data() {\n    return {\n      isOnComputer: true\n    };\n  },\n\n  methods: {\n    toggleDarkMode() {\n      this.$store.commit(\"toggleDarkMode\");\n    }\n\n  },\n  computed: {\n    isDarkMode() {\n      return this.$store.getters.isDarkMode;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA;AAEA;AACA;AACA;AACA;AACA;AACA;EACAA;IACAC,YADA;IAEAC,cAFA;IAGAC,aAHA;IAIAC,OAJA;IAKAC;EALA,CADA;;EAQAC;IACA;MACA;IACA;EACA,CAZA;;EAaAC;IACAC;EACA,CAfA;;EAgBAC;IACA;MACAC;IADA;EAGA,CApBA;;EAqBAC;IACAC;MACA;IACA;;EAHA,CArBA;EA0BAC;IACAC;MACA;IACA;;EAHA;AA1BA","names":["components","VideoTopMenu","VideoCarousels","AllSortsVideo","VideoTV","FooterVideoHome","created","mounted","NetlifyIdentityWidget","data","isOnComputer","methods","toggleDarkMode","computed","isDarkMode"],"sourceRoot":"src/views","sources":["VideoHome.vue"],"sourcesContent":["<template>\n  <div\n    class=\"videoTopWrapper\"\n    :class=\"{ 'light-background': !isDarkMode, 'dark-background': isDarkMode }\"\n  >\n    <VideoTopMenu :isOnComputer=\"isOnComputer\" />\n    <VideoCarousels :isOnComputer=\"isOnComputer\" />\n    <VideoTV :isOnComputer=\"isOnComputer\" />\n    <AllSortsVideo :isOnComputer=\"isOnComputer\" />\n    <FooterVideoHome />\n  </div>\n</template>\n\n<script>\nimport * as NetlifyIdentityWidget from \"netlify-identity-widget\";\n\nimport VideoTopMenu from \"@/components/sections/movieHome_section/VideoTopMenu.vue\";\nimport VideoCarousels from \"@/components/sections/movieHome_section/VideoCarousels.vue\";\nimport VideoTV from \"@/components/sections/movieHome_section/VideoTV.vue\";\nimport AllSortsVideo from \"@/components/sections/movieHome_section/AllSortsVideo.vue\";\nimport FooterVideoHome from \"@/components/group/movieHome/footer/FooterVideoHome.vue\";\nexport default {\n  components: {\n    VideoTopMenu,\n    VideoCarousels,\n    AllSortsVideo,\n    VideoTV,\n    FooterVideoHome,\n  },\n  created() {\n    if (document.body.clientWidth <= 900) {\n      this.isOnComputer = false;\n    }\n  },\n  mounted() {\n    NetlifyIdentityWidget.open();\n  },\n  data() {\n    return {\n      isOnComputer: true,\n    };\n  },\n  methods: {\n    toggleDarkMode() {\n      this.$store.commit(\"toggleDarkMode\");\n    },\n  },\n  computed: {\n    isDarkMode() {\n      return this.$store.getters.isDarkMode;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scopde>\n@import \"@/global-styles/mixin.scss\";\n@import \"@/global-styles/colors.scss\";\n@import \"@/global-styles/typography.scss\";\n\n.light-background {\n  background: $light-gray;\n}\n.dark-background {\n  background: $dark-blue;\n}\n.videoTopWrapper {\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}